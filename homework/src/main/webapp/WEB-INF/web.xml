<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
	version="2.5">
	<servlet>
		<servlet-name>ClientOverview</servlet-name>
		<servlet-class>com.companyname.server.ClientOverview</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ClientOverview</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddClient</servlet-name>
		<servlet-class>com.companyname.server.AddClient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddClient</servlet-name>
		<url-pattern>/addClient</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeleteClient</servlet-name>
		<servlet-class>com.companyname.server.DeleteClient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteClient</servlet-name>
		<url-pattern>/deleteClient</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditClient</servlet-name>
		<servlet-class>com.companyname.server.EditClient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditClient</servlet-name>
		<url-pattern>/editClient</url-pattern>
	</servlet-mapping>
</web-app>
